    "glob": "8.1.0",
    "graceful-fs": "4.2.11",
    "gray-matter": "4.0.3",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "13.3.0",
    "npm-run-all": "4.1.5",
    "p-iteration": "1.1.8",
    "parcel": "2.9.3",
    "plop": "3.1.2",
    "prettier": "2.8.8",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.5",
    "tsup": "6.4.0",
    "turbo": "1.6.3",
    "typescript": "4.9.5",
